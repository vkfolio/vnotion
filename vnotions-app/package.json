{
  "name": "vnotions-app",
  "type": "module",
  "private": true,
  "version": "1.0.0-alpha",
  "description": "VNotions - Local-first, privacy-focused knowledge management application",
  "main": "electron/main/main.js",
  "author": "VNotions Team",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:electron": "electron .",
    "dev:full": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run dev:electron\"",
    "build:electron": "npm run build && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac", 
    "build:linux": "npm run build && electron-builder --linux",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "ai:setup": "cd ../ai-engine && python installer.py setup",
    "ai:start": "cd ../ai-engine && python start.py",
    "ai:dev": "concurrently \"npm run ai:start\" \"npm run dev:full\"",
    "workspace:init": "node scripts/init-workspace.js",
    "clean": "rm -rf .nuxt .output dist-electron node_modules/.cache",
    "typecheck": "echo 'TypeScript disabled - using JavaScript only'"
  },
  "dependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@pinia/nuxt": "^0.5.1",
    "@primevue/nuxt-module": "^4.0.7",
    "@tiptap/core": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@tiptap/extension-bullet-list": "^2.1.13",
    "@tiptap/extension-character-count": "^2.1.13",
    "@tiptap/extension-code-block": "^2.1.13",
    "@tiptap/extension-color": "^2.1.13",
    "@tiptap/extension-document": "^2.1.13",
    "@tiptap/extension-dropcursor": "^2.1.13",
    "@tiptap/extension-focus": "^2.1.13",
    "@tiptap/extension-gapcursor": "^2.1.13",
    "@tiptap/extension-hard-break": "^2.1.13",
    "@tiptap/extension-heading": "^2.1.13",
    "@tiptap/extension-highlight": "^2.1.13",
    "@tiptap/extension-history": "^2.1.13",
    "@tiptap/extension-horizontal-rule": "^2.1.13",
    "@tiptap/extension-image": "^2.1.13",
    "@tiptap/extension-italic": "^2.1.13",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/extension-list-item": "^2.1.13",
    "@tiptap/extension-ordered-list": "^2.1.13",
    "@tiptap/extension-paragraph": "^2.1.13",
    "@tiptap/extension-placeholder": "^2.1.13",
    "@tiptap/extension-strike": "^2.1.13",
    "@tiptap/extension-subscript": "^2.1.13",
    "@tiptap/extension-superscript": "^2.1.13",
    "@tiptap/extension-table": "^2.1.13",
    "@tiptap/extension-table-cell": "^2.1.13",
    "@tiptap/extension-table-header": "^2.1.13",
    "@tiptap/extension-table-row": "^2.1.13",
    "@tiptap/extension-task-item": "^2.1.13",
    "@tiptap/extension-task-list": "^2.1.13",
    "@tiptap/extension-text": "^2.1.13",
    "@tiptap/extension-text-align": "^2.1.13",
    "@tiptap/extension-text-style": "^2.1.13",
    "@tiptap/extension-underline": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/vue-3": "^2.1.13",
    "electron": "^32.0.1",
    "electron-is-dev": "^2.0.0",
    "flexsearch": "^0.7.43",
    "nanoid": "^5.0.7",
    "nuxt": "^4.0.3",
    "pinia": "^2.1.7",
    "primevue": "^4.0.7",
    "primeicons": "^7.0.0",
    "simple-git": "^3.25.0",
    "uuid": "^10.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/test-utils": "^3.14.2",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "electron-builder": "^25.0.5",
    "eslint": "^9.12.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "tailwindcss": "^3.4.13",
    "vitest": "^3.2.0",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.vnotions.app",
    "productName": "VNotions",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      ".output/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraResources": [
      "ai-engine/**/*"
    ]
  }
}
